			

0|ֿלךמסכמםםסלנסעסמסןףףןנףןףךןמנןמעננףכממסלןמםפןךעמנףנססנעןלץםסןןנסםלסרקצנקשקרעקפצץרפפצץעעקקצץרףפףץןעץץסףפצץסמרץמףממפףפצץקפףףםעקנןמj'

												/׀ימךנןכןנןןמםןךמנמעככטססםןלןנמפסףלפףמענמלפץסףסנןמסנםעמממעמססעןסעןסעצץצצץפצקשנרתרסץץפץףלשפץףקץפעפרןעעףנצפףןמןעףץסקפרצפןסףץסןרסןןכֶn)
	
									

1־חסנלמץנףעמלןנסנףךסףמנססנןעסףיפנץףםחנכענםנןמסףממףךנעסןצסמןכמסננףנפןקתפתקפקצןףרץףץץץרףץצנתףףרןףשפףצףםץפפףקנףפףסקףןףץפפכפפפקעסצצננןֹp)							
	
1zֿןילעמםנססןכננענמיףלןכנעםףןכעםמסעכמלףצןךעעםקנסנפסןנמניןעםףעססצמפננעקץרסתרעפקעפפףפץףףפסףףםףפסףףעץצםץףפךפעפפןןץךרעףרקןצעלקפמןנןנססכֻq*
						
