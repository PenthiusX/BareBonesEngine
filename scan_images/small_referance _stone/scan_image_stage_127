ןלימיגדהזזזטזיטיילממםנסןפןףעףףףסץפשצקקפשקשרשרקתצתתתתתקץרפד¦O				(kֱךנףעקצצצקעץצפעצפפפפץץץקץצסצץףץץפצעץףףקצספץעפפעץףמףעעףננסמםנסןםלככםכיךבבגוווזוטטטךךלממממנעסףענפפץצץפץףררקשקרשששששקשקקתץצץה¦O
	*oִךמעץסףקצררקץצפקצףעףעצץףצקעפפץעץקצקףצקעץףץפפעעקעעףנסעמעןמןןםןנמםלמלכךכגדדגגזטחטחחככלםנלמססעסףףפץףצףצפףרפשררצתקצרשץשת‏צפה§N
+lֲםעץץרפקץפרפספצץצץצרצףףקעץףפפקףפפצץקעצפסץפפפעעסעפנעססעןןםלמןםןמלימטםייבדאהודהזזטזךךלםךנםסנןעפנעףףץצףץפקרתתשקרץקשרתשקשתתררעד¥M		)h¼יףץףץצצצרףרץפץשץץפצץצפפקףץצפצקסףצץץקסצצסףץפצפנספףספנענןנסלמןלממכםלחלח