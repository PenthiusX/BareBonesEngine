=לסףססעפמנעססץנץפןעןפןןמסמסמץנעעןםףלעמנןפםןעןפנננףנסססספטסףנמנלןתשתץקףסנץץםקפשפעצפסץסתץשץסספקץמססרפפסנץףפךץנעעץףסמףץרסספףפפןנםז§K
			

@”נסנעעננףםןנןןמסננםמףסעמססנלןנליעכףפנןסמףנעעמסןמכןמןעםסףץףפעםעמנשפפףשףתץףץעקקעצץעתקנסמףפץןףץעמפפעץננקץצססצעםףןמץןמנענץץפכפףננפלח©N
						
A–ןנכנעןןמןסןנןנססענעפנמסנףלנףכעסןלןפפנסענןןנננעלפמעמףנסמנעםףסנסםרשצקפרצרףצקפשקשצמקץסרנפןצעצקצץףעצעסץסףקפעפנפץעףסעץםפץףןפףףןפןח×N

					
@˜לןףפנלסמףמססףסמלןןףסןץףףעךמנןעןכןעףלףעססןלףטןפכנסנענסנפנןננמףמנקשררקשצףצצרררשעץףפןףסצצףפעעףפמףעסצרץץתפןרסעעמףץסןףןסמפנמפםףןננד£K							Bנןסנףךןנךעעןסןנמףנעעףענםנםכעעךמץמןסףעסןננספףףעןןץץןףמעםףמףףמןעסקץסקעמשצפצץףץףספץםפףפעצףפןעצצקןפעץןסםסעףעןףללצעיענףמןנעסןצןפפנס›F
			
C™דעםענעסענפזףםמסםףףפןעסצננןסמןףמממףכפנפמפףלףסעףעףזממםסכףמןםענעלעסרפץצמףצףרעעסףפתעץנסעןץצףפצפרפ‎ץמשץןפקףצץנץפךפתףנעפץנעםףףצץעסףםױ@

			

E˜אמםנענסןכעףסףפןןץסיעכףעססםןמסךנננסםנפפמספןנץסץעננןןןמסמפןפסססץןמקפשרפצרעתרפףפעםפץעףעפףשצעפפעןעצפםףצץטמנףניץעףצףםקעצקעמעענמפףמױ?					

?–מםםנעלמפמןססןפעסענסענףנסממנץסןעקנעףפפםלםפמןנסנףןמףנמנסףסףףפסנץףשקרעקקנצצפרקףףןצקץץףפרפעץעקרפעףצפףםצףעצצעןעףףסעפנקץנצןףץףנףןנן–E
							
