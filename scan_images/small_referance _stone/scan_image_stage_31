מםלודדזזזךיטםךםמממנםנץננפעסצעצצףררקרתרץףקץרתתת‎ששתצצתררתקשתצפז¨P			1wןעעץנצףץףפנסקץסקפפצףףצפץפץץצעץפףצקסנץפףססעץספףעעףסץףנןףמסןןמנמםכגוווטזזיטיליךןםםנננפפעץפץףצפקקררצשקרשתשררץששקתרתתתשקצרצףז×O
	1wָכסנצפעצצץפץסצץףפץצפקףפרףצפצצףצץץעצפץףףספףףעעפףנסמענןףסןןמנןםםןךםבהדהזזייךטחמככןלמןןמפףנפףצקפקשרפצספרצערצרתתרצש‎תקתקת‏ץתעב¢J
	/vָםעפףקרףפצפףפףץקקפצעקפנפץפץצץעץפפץעעקפףפףעףספסףןףסעפףנףסמממלנמכןךדדהודטוויזךלללעםםעמסעסעפףץקץפץעץתפצ‎קששררששרתתרתתץתתרשת‏צשצרץבF
		/vֽמפףצףץפצקץףץצפפרצפףףץףעצףפצףפפפעץץעץץעףקףפפנעץןףעןסףםסעמןסלללללכגבדדוזחחךכטילמלםםמעםןףפעפפףפפררצשרפרררשתצתשתתצתרשתשצקרשקצתףדH
		0wֻךעפץצףפקפצץףץץפצקפפץףפקץץפףסספקקפפפעץצףצפסנץעףעסףסנעסןנעםכמכלמיךדבבבהזזווךיחכימןםמףננעףףעסףצעצקץררפקתקצת‎תתתתץרצרתצת‎פצקג K		