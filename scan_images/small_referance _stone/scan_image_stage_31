וחוטכייללםנממףננפףנפףץרפפצפקשץתשרררתקתרקתצת‎רצרתפG
	1z־ןץףפצצצקףץץףעצפצץפפצפפצץסץץצשפץצסץפפףצסעקעףפןססנסנממנלמןלכמככםיכדבבההזיחךטטיכמםנםמענעעעףפסעצףפץעצץשצצץרשצרת‎צששרשץע™F
		/yֻלףפףקצסףצףפץפקפפףקסץסץפץפפץפצצעצץףצףפפףעסספעעענףןממןםסלםםםכםילךטבדזהוזוךטזךייםסלןסןנףףעפפצצפץפרקששקשפקרתתתר‎תתרשץןB		0yֽםעץרףקצףצקפקףצצץףץץפצצץקצנףץפקקסץפעעעעץענפעעעענסעסננםןמםןמלןןךןך