									
/yֿךלללסםללםמלסנעעכססעעסןפנסךסמננםסעןעםםןפכןסלףסטסננפעססססןמץלףןןעןלמןתקקןצצרףצףשפקצפץצססשצץקקסצעפנעפצנסקפפעםצקםעןםףפףץרקףצעלעצמןײ~/


					
		

/xֽךכלםןםםממסןםמםינןמפךךךנןםמכסןםץןעןףענףןסלףקסעפנמססןמןלןמענענפמןץםנפפרצפקףרשצקףקקתספרפפץךקקפסתפעץעצססספןפקעמנמנפצןשפקקףמףעפףמצףםםׂ|-


								
+nֲהךןןמלפףסססכןענסץינצםןףןנסנסץיףספעןזנםסןנממןנענלסכמנעםפףםםםמנעןסףףןץשצשקץץפסעשקסצצףרץפצסרףפצןפקפץץףמפףץעקעעפפןצפמףצףפכףפצץעעץץסםׁv-

										
	&\«נךכסעלמסנןםננעןםכסכנךנפםסנכסןמןפיםמעפסינףלץנסןצןמעםנכןנמףנענץןסמססשץקףשקפעצץףףץעףצעעפףסןסעףסעץפצןפסקיעפפףעמפםצסץקץסץסםצעסןמסןמנֻq,


									
	#M—לממממלננפעלנןנףימםחנעכמעלןםענקכםעמסלךסןכספנענמךנעםמנססמענןמןךםנכסץקץפקץשץץצרץפתצנעקענצףףפצץץףףרעץץףצץץקצנפמנפפםפקץפףעףץעעמףץםנּs+	


	
	
$Mןנןןןןפלסנףננמעסמןףננסנמםןכנכעסןפץןסמססףנעףךסףךפסכףסןץנללעםןמסנסענרצקףןץרץשרצשקףצפרסץקצעעקנררנצפץעץץרףקקץצמפןסףפלצסףףסעץןענעףמןֹq,

	
							

#W«בלנפמסןנףםסףעמןףכףכםצסנעםםףממסמעפמםפםספנסןספןנמןלנןנלסנםןןןנעףעמץנעתףףףרצקערתפנרפעצעץףעפפףפשצעץמעףףסץעקעצפפסעענןפקץפצפצפעעעןשמסנַo,

								
'd¹חממנלמננעסןעפלןימןסןנץןסקןנףםעסןנףענףלסמעפעעססממנכנףמןףלףעענעניףןףףררעץעףשרפןפענפצץףץקפףץעפקצץשץףנץםשספנפענעןנץןןסלמקנסעמנענננניַm(
						


*oֳיסמננןננםםעמךמןמנןינמננכןמןפענםםסןענסנסמסןפסנעענםעיןסכןםןנמספףןנצןנףעקסרפקפפץסרפףנץשנןףפףףפעקלץצנססלעץףץןעםצםףמםצףםןפנעפףףעץץףכֳk*

							
,tכמנעםמסמןנןממףנמננעסםספנםנלןפעחמנןףעןפססנססמןמףןןפמםסמלעפםמץעסעלץסעקפשץשרקץשקףסצסתצצפעףעפצןפשמפףפפףררמףקסרצעפפףסצנסףסףעעץףרעףפמםֳm)



				

,tֹךםןסןעסןםפםנןמןסנענףמטמצסעלססןלנננממעלןנןפסןםפכםעץסןסלןןןעעףםסןעסעמףקץרששקקצספצצץץשפפצפצפעץפעעפץםףפנעקנעסעפצץסךןנפעפסןףץממסערףןנֲl)
	
			

+sַלנןןסנןןנםפמעןנםןןןנםמףנסץלןףןןעעןסמעםכננסנפמנןןלססןץלנסלמסספנלםסנספפרץףצמצןקץפעצמססץףרנףקמןצסףעעצןמףנךנכםץפעעןעץסצףמעסעפץףמףחֳj'


									+rטסנןןנמלקסעסךמצםסעןםןםףסןןךלןץכןןלמנםסעיןנןמעףסנמןסמןעןןףלףןללנסכןמרסרררששףקץצצרצסקרפצסצףפשצצץץקץצעץףףץצסץןןףףמעןנץעףצפקענפןנסעכֱg(
	
							
*qַמסכסןמננסנעןעמכעספממפםממןנססמםעמנןנעףןעךמעממעענןןןסםןעסנןממעמםםלןךסרפצששפרקץףףפץףףשתסקףןפצךפקצסעץץצףנףםףןףפץףףסץפקמשצננרנעענמסמך¾g)


						
+qָםנםמנכפנמןףמסץלעפכןםעמעסםנףעעמפענךסמנלסספפנפממפןץןנטןןנעןנףםףץמעסנסרצףקפףתסקרסעצעףץפקץםפרצקףקקפלפשפשפפץפןפספףעםעערץסנץםןצנעמיµ_&

	
					
+tּמכממעףףעיםמסעםעםןףןףעענפןךסףףקנססעןסןםלנסץעסןלפפףןממטןפממסמסננקעכפעקצרצץסרפפצסצצמץסמשץנסקמרףפסףףשפקץפסףעפעצסמנפעףסנץננצפסםןנףםםה¬[%						
+tַנןמןףםץמםףסנףפנסעלףננסןסעלפםעסנלןננןססףץנףףעפנפלףךןעטכפףסעסןנןסנעסןקרןרצצץסצץץףפנןסעצפץערמפםנספץףקףצץףצץצןןננמףפעפללטןנסנעלמעסד«X'


					

,sַךמםלסמנעםלפלססנםסמךמנםלןנפםסנחםףננעסעףמןןסןמןףנםלןםנץףספנןםנמףנףסםןררשץסצפץץרשצפצקפצעשצנסףקץקצעפעץעםסעפפצםץןפמכףםןץםןץםנספנץעספו­[&
	
						
+qַלנםססממףלןסףנםעלפםפעכמסןנןמםפןמפמלףףסלץןנףסעםמססםםסנסעסןףעןןןענעעףטסצפץפרקפקתצפפץרףץץספףףףנץצפצץףףשץכץףנקעפענעףעעמפפפףצספןצףססמט´`'


						
,oֹןסםפסןנםענכמעלסןענמןץצסךםעמנןןנסםפסןמנןמסןנןמעפפןךעןנןמםיעמןמענעמףקצשץקעשעצסערנפשצץףסץסץעפןףףלצףפנץסנפצןרןנקסץצצפסצנףףץקםפעןץפמכ¿f'

						
+rֶכנלנעססנלסףעעפנםמנלנמןנךנמםמנםםמןנןללסךססןףןנסענןםסעפםנעננףענףסןףןןפקרשצתצףץףףרצפףקכנףשץרצקצפצעעצעעףפססעצפףרןמסץףמפףןקפלללנעקסןכֹo)
						
*mֵיססנפףסףךסמנןנןןענמננכסףןנןסםסלןעןןפנממםנסךספןעצססףןנעןעףעףףןמסןץעע‏קצרצתקרףףעקרץקץרקשקצץסעקסנףץסקץעץםפצמפפןףפןץצןנפעםספצפםעפסענq)						
*oֶםןלסםססלנסלנףםענףףססםעןנפענצמלעלןןםפססענעףעםלסןפכננץםנףןלעףןףמםףעףץרףצפעקפתשצפעעעקרצנעפעעףקןרעעצספפץפעףעצעףףףפףןץקםסםרפפעץשיקפנךr*

				*qֶךסענןספססעפמנעססץןץפמסנפןןםסמנמצססעןםפכעמסןףלןסנץסןנףנסנסנףטנפנןסםנשתשץקףסנץץםקפשףףקפנץסתץשפנסץצץמסנשפפסנץףפיץנעעץףסמףפרענפףףףךֹq)
							
+qמממןסעסנעןנףםןנמןןננעםםףסעמעננכןסליסלספנמעלענעסםננןךממןסמןףץףפףלסםנתףףףרעץסצעקרעץצסשקסנמעפצןףצעםפפסצנןרפץססצעםףןמץנמןףנפצפכץףמלַo*
					+rָןםםןנסלנסנןםנסמנמןעסנןעענמננףכנעךענמםמףפןסףנןנןןעעלצמעןףנסםנףםענמנמקקצץקפרעצרפשקרצםקצסקסףמצסץקץץפעץעסץעעצץספספפףףנסץמפץףןףעסךֶh'	
			
,pַמסמעםעףףנמןמףמנףעסמלןנףסנפףפסימןמסמכנסףםעענסןםעחמףךןןנסנננפנןןנןעמףרתקרררצפץצרקר‎רעץעפןעסצצףץסעףףמפסערקץצרץןקסףסמפץענסןסםץנםףלסיְd%	
			
+qָךנמןףןננעךנןךעסןעןנןסנפספעןמסלכפעךןץמןנףפנןענספףפעמנקפנףםסםעמףףםןעסקץסצעןשקפץץפץףספץםפףץסצףףסףצץרםףסץמעלסעעףנעכלץעיסנףממסנסנפמסי½b%

		
,nֶלנףנףםעסעעפןףזףםמסםףףףמססקנסמנמנעםממףכףןפמףעכףנעףספזןןםסכףמןמפנעכףערפץץמעצערסעסףפתספננעןץצףץקץקפ‎ץמרץןפקעץפנץףלפתףסעפץנעםפףצפסך÷^%
	
		
