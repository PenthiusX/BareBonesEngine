פפעפפסףץנםסףפףףפעצרפפרףץשץףץץףצץץצצששתשקקששפתתתק‎שקתרשפתפשץפרקשץע׃4		O¨העסנצףףפץפצסעצפפקץקצעפפפץץסץרפןרעפףףףפעעץםססףעקעצןףףענףפלץצןעפסףסעפףענסנמףסףנפףקקקץנפףצצצץקקקפשפשששצרץששתשתץרתףשרתרשפקףצרץקע׃ƒ3
		P¨העצסןעספפקפרפפץסעפףףסףסץףעעסץרססקפץצףץץספצפעפעעפףפץנפקעףפנעפןץףסףענעסססמםףןםףפעעפקצףץססץרצקצץצררצתררררקקקףץררתששקצףששצרתקפצשצץױƒ5
	!O©בסףצףףץספפעצפעףפץץרףץףעףפצץףסץץסץקץץעץץצץפץעצץעףפףףףסףץעפףןףץסףץנעץססענןמלסענפףפץפץשץצתצרשץקתץרתצצתשצשרקרקקתץףצשרררתקרתקפקףױ4

P¥דנפףפעףץצפץצףקספצסףפנצץנץץעעפןפצץקצעףץצץמףצץעפפפץץפפעעץנפףפעףפןןעמעפןףלןנןןענףספפצףפץףששפצצצץשששתצתרשששפתקתתףקתתשררקשקקקרעׁ2		
O©גמץףעקעעפעףקפףצסףצנצףףץפעץעףעץסקץףץףפפעףצקפעפףעצןץרפףפפסףףץףעספנעףןעףממכמןןמסףסףעףפץקרנצרץצקץרשץרצקפצצקרשרצתתקששקשקרשקשץצשתץן־z/


	O¦הןףצעףפפפץפפקצצרפפפעסצפצףסעץסצץסףץץעפסןרפפףנפץעסצפסעסץץףץץףסץנסףנףץסעץמןכםסםסנסעפספףצףצסץפץקצץץצרץרצצקףרצר‎תשרקררץתששרץרתתפנֶs,
	M¥גסנמפסץצקץפףץצצפקףצשפפצעעףץפץףףפןנץףפסעפץעפסעצענףעעףצפפספנץנץעעףפןעסננםןםםןסלנפנסעסףקףץשצרקפרתץקשפעשץרשרשצץשצקרצשתרק‎ששץי¿g+	M¢בנפפסצץפצפצףףצקצףצץצצנץעצפןףץץץרסץצצפצפףץצפקעףפנעץסףץפנפףףעענףףןסעסעננמממנמלעמססןעעפףפפקרץרקצשקצרתקרקשרששתצץשץפצקתתשתקששצט¸e+
	O¥דספרנקפףצץץקץפצפףפפפפקפףצספפסץעץקקפרץפפץףפעףףףססףףץףעץפפספףףףףפץמעססףסךככםןםמןםעעעססעפצפפקפקקץקצפקץרררצשקצרצתשקתץש‎קקרצרצלְj-	