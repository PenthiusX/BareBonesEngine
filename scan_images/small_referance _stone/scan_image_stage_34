יחךךךללמןלסןןעץענפןנץעקץץקץררקצצרקץששתשץרשקתצתרקת‎ץררשררץששצףװ‰9		4z־כףפעצפעףצץקספרפנףסעפעץעפנסצעףפץספףץצעפצפץףספפסצץעפסעננעמפנףפןסננךיטכךלכלםממנממענעףףעצעףקעפשררתרקששרתקצצשתרקצ‎פץתץרקתשרשרףםּy2		
/sֲןמסקעצץפפרפקצץסץףפקפפצנףפצפצסקפעפפעסץמףפספסנףףןןףסףעענףעמףםפעמעסזטיילילםםםןנמנסנעסעצסףקףץרץשץפרשצששפפתקתקרצצשרצרששצפר‎ררצמִn.


/vֶןסעףץףעץףפשץעצפץפצצץספףעקצספקסץץףנףפפפףףקעףץסנץענץענענננןעמםנןכסוטכהטכיכןןנסמנףנעפףצףפשץףרץץרצצשקצרפרשתתתרתקףצשתףרש‎תצשץמֵr.
	
3}׀םעעץפקץףץףפףץףץפצפפפצץףצףפףצצצפףענףפפץקעףפץףףעפףסעעעעננסססעעןםממטזזחחחםךלםמןןסעפסףצףרץעףצצקקפשצףקשקשץשרצתתששתתרתץקששררתצנֿ}3		9‹ײףפץפעצקףףץצפקקץפףפצץףץפצצףעףפעפפףסעפץףץץףעסנףפנעפנסנננעסנסמנןמםךגזזיחחטךילככןןןנסמנעפץעפפץעפקררץקרצץרתתתצצתתתרצתר‎שרתרתתרתרף״<		