בבגהההטזטחיטילמןנמןןמפףןףףפקץפףקקתקרףרתרתתצתרצע״…;
			0xןףפפפפרצרפשףצץקקץףצפפפףצצצץץקצפעץצקץפףץףעפפפססנעעספףנעעממןמםנללםכמחמח