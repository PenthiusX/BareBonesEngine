יםלדוחהוחזטטחכלךכנםמסנעעףעףפץקפץץפצתפקתקתרשרשקרתתרששץתתתקתתץפםֵo+
	/{ֽסנפפץפרפץצץץץףץרץפקץפפפץףסצץףפפץעףעפצףפצףסרסףפסףפןעעמסףםססםםנםלםכככבבהדדוטוטייטםלךםלןנמנףףספץףצףשקקתשץקרקתשרררתצרשששקקשרץםֽu-

		/yןפףןצףפצץפרפץקעצצפץקפפרפפקףץץףסעףקקעץףעץקףפץענפעעסנעססעעןמןמלמלםמככגגגהוזטהטלטחלךלמממסןסנעעףפפצעץץקרקץצתרתרש‏תתץרצרתקרשם־y/	