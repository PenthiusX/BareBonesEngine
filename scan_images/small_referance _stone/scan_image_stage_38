ןננסטיכךךךםכךכמןסנסספסףץץנפצףצקצתרקשררצקץקתשרתצקפרתתץרשששקקףו´`(
	 M םעסץץץץפףפקצץקסףצףץקץפסנעצפפפץעפעעץספנףץףסנפעףסמנףסנעסעץןעעסעסןסוזךטךםכךםלןעןנסמעפעץףעץצפצקשףקצצררשרפרתשץתתפששתשצשרתץשצרתרץך´d)
		
 ?…ָזןעףףףףפתצצצנפץפףרצצעץףצפפףףץץפפפנעצפףץסץץףףצעףףמץפנעפסןססמנןמםןזוטטייליכןםנסנפעספפפעצצפפקץקקצקצקקצפתקשררצ‎שףרתתקקרפתץכֲo-		
3g³הנףצףצקףץפףעקסרפפפצףץץפקפףףצצצפףפפףעצףץץףץפסףףףפףסעעףמעננענסןמםןחחזויחכיכםןךמסמעסףףףצפפףצקצרצרצץשצתץשקרקרשרתשתרר‎שץרתקתשקשקסּx.

,d±זףעעץפפצעץצעצצפץפפץץעץצפפפנסץספצףספסףפץצץסססעצףנסעמסץןנפמנםננסכלהזחוזחחכלךכמלממנננןסץףףץספץצצרקפרקצצקתשררפרתשקרתר‎קשצרתתשץףׁ{1
	
2qְטעפפףףפפקפףרףץעץצףפצץףעקףץקצץקףצץפצפסץפץעףףספףףץןנפןעעןססםןנמןןכהגדזזיטךךמלככםנסםנעןספץעץףקצףשררשתצצףרצתררשששתפקרשששרקתתצסׁ~4
	
=‡װןנעפעקסצפץסעצץסקפץץפעפףץפצץץעץפפרצסןפףעסףפץףפעףףסעפעננסנענמםםםןםגדוהחחהטיכםכמםלםסןנפסףפפצפפערקרררתרקשתשרשץרשרשרתתתרקצקתשצעװ‡4
	
D™ןףףצפסץצפףפספצעפצץפקעפקפץפפץףץצצעצץפףףספףפףפףעסףןסנמףנננמננםםמךמדהדגחחהחטזילטכללןןמנעסעץףצפצררקץקעצרץערקקתששץתשרשרתצרסײ2	
Kנףעףתקץץףפץפףקצפץצעקעעץפפפץץפץפץץףףקפףפפעץעפנססףעפעננסנןנןםמכלמםהבזהדהזהיטחככלםמממףסנססעפפפקפפפפצרץרצתררשררשתרשתצתקתקרץקפײ„1
	 M נענקףץרץצץפפקפערץפץעףףסצץףפעפפפנףצפפקעעשעףפףסץמפסןףעןןפמננםמןךלכגבהדדוחחךךחךםםםלםמןןנעףעפפעצפררצשרץקשקרשקשששקרשתשצרקרץשעׂ4
	O לצספצףפקפץצףצקפצקפץקפץקץפפעסעפקרףףפעפקףפפססץעפפנףסןעעםנמםכמללןיםאאבהבוזזזךךטככיםםןןמסענסףעףץץסצץקקצצרתשתקת‎תץקצשקתץץףױƒ5
		 R£ףפפץףץפץצעץעץצרףץצץץעעפפףפפץפרפצקפצפףץפפעףעעעףףענסענענלמןכןלכםךךאאבהההזוזחיךחךיממםנננסססמץפפעפףררשץתשרצקקקתשת‎קקשששר‎קןװ2	 R¦דןףץצעפרפףץעץפףץץץשץץןצפףקץףץףפפפצץנעקפפץנפעןסעףנעןןנםןסםמנככלםכךבבדגההחדזךיזםךםןממסננףסנצפצקפץץץקששרתקרששקשרצתקצץתשעװ‚3			#T¦נפפצצפצצץרקעפףעצצפץץעצפץפפפפקפץצףץףףפפףפצעףסמסנססןננןמסנלמלכככילאאבגגדחיוייזלכממםלסעןעףנעףפףצעץפףרפרקרצשקצתתתצתת‎קתררצןׂ‚4
	
"P מסץסרץנףרפפץץקצףעץעפעףפצפפקפץקפפקעצצפפעפעףענעסעעןמםםמסםמןםיםטךךטבגגוזזזטיטלליםסןנעןסצןעץעפרעפפרקשקששץררשקצ‏ששתשרש‎פןֿ€2
		 Mןעפץעשץףץרפצפפףץףפרףפקץקץסףצפקקסץץףףפףפעסעפעעףןססןםןמןמםמםךםמימי