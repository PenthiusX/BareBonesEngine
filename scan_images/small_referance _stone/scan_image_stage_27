גוזזחזחםךלכםםממננסנףףףץפעקףקקשצרתצששרתררקקרתרשרקרצרשקצכ¸Y 
		'h¾כןפקעסץףפפעעצףףץףרצצץץצפץעצקסצקפפקפצפנסץנפפספפעפןנסןמנסןנסםןממממגדהדחוחיחטטלכלםמןםנענףףעףרףץרקרקצסצקץףררקצ‎‎צץ‏תררת‏‎‏שצפךµV 		'f½םפספרצפץףפקףץרףצצפץרסעקףץץפפפףץקסףףץץץףףףףעץמףסעעעףףפןמעםעםםםםךלבוזהוזויטיללכמםםסססףןעפעפצץצץףפקרפקתרץרשצששרשרתקרץקתפררו¯T
	&e¿ךפפפפץץקקפץקץפץץצףעפפעעץעץץסקפסףפפפפץץעצפפףעףפןפןןעסןנמםןממממםלידבהבזחזטלטטםללןטמסןןננץפעפץעפשץצתץצשצרקקקתפששתקתקצש‏ררקרףט©O		'c¾ךספצףפפץץקףערפצשפץקפפצץקקףףףנפקץפצסףקצףץפסנסףפנספנסעסןעםלםםםםמכםאגבדבוטזטיייטטלםןנןמסענסףעףקפףצפררצקתשששקת‏שקצקתררתקקפה×M
		&f÷יףסץפפפץפפפנפקץץץפץץעפפףפפעצפקפפףץפעעץפפפסעעססףספףןסנןןסםלמלילכלבבדדזוגחחחיליםלםןנמנףןףנעפץסצפקררץצתץשררשרתקשתר‎צשתקתתשפו¥N		&e¾טעץפעץצצףץפצסץץףצשפקסץץפץץפףפץעפקףעעפפפץעפססענעףנססםםןנןןםמלכןייבאהגהדזחיטךייםםכממןסנספספפקקפץפפתקרתרשששתרררשצררתשרקתשרו£O		'hְלףפרפקצפץקצפפעץקפץץפץץףצעפצקצפצפףץסץפףעףצפץסעסמנףמןןללמלןןםמםטךלאגגגדזוזזחטךליממםנננננסףףףפףצפצעץשףרצצתקקתת‎רתש‏צשרתקסד¥P	%f»ךפעפצעעפץצץףץקףפפפףפנצףץצץץץצפץקףפצעץפנץעעץסעסענעמלןןםםלמלךכךמךיאבבאהגהחוחךחלכךכןןמנןעננעעעץץצףפערששרקשפקששרת‎רקתרעה§L		'g¿יפצפפשעףפפפשעצקעפץץףצצצקץףפףפרפעקפעץעףץעסענפפענפןסנממןלממלםלכךכך