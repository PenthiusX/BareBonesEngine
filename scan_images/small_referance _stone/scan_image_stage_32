סססףפעףקפקצקצקצררצרתץשרששתתתצץשץפקקתתששקשתתץרתץעב—?			?‰׃לףפפץףץץףעץצפץצפסץעףפעצעףצקףקפפפץעפףץפפסעפעפפסץפץףפעעעעףףנףעןףןךככםןםנסןעעףסעסףצעפקץקקפץצףקצקרשקתרפרשצשרשץק‏רקשקתשרשצתנד–>
	F—נפףקפעקפצצעףרףצץצפץפףתףעפנקעףפצסץףעףץףעצעעףסףעסףףףעפףץעססףסננמןךםםלךכממסמנעסעסספףץץרץקשקפתרשקצצששתשרפקרצתקררת‏צרקץתצשררףג›B		P¥אססףפפפףפפקפעףףץףץףץפףספנקפץץפפפעקץצעפץףסףספצפנףףפפסעפנסעפסעןףןןםחךלךממםמםסנףפףענעףפץצצרקףשרשרצקרשקשקרצצתקרשתרררשץץץתצץשתפפגE		![°טןנצףפץפקפףצץפץפסץפעפפעפקףפפףץצףפצףפצףץצףףפעעעסעףססףנףפןספססעפנעכיךיםימעמםעםנץעסףנסףפףשפצרפררץץרץרקקתקצששתשרצץר‎קשץתצקתתפג E
	%`µחעסעקעףףץקקנףרפסףעףףנצצףסףףףצפקעףפצפעפפפץסצפפפפעףףננףמססףסענסעןסזחךטךךךםמלםןןןףנעפעעקןףרעצתץרקקתקתץרקקתק‎‎קצ‏עק‎רצ‎קר‎קתשקץע›F			