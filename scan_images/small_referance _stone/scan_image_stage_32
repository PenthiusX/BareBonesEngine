וטטזיטילךןסמןעמנףססץסצתףץשפץרץצקקרעשתרתשרתץףצרפשר‏תקתתפה¦J
	)l¿לסעצפפץףץצץףצףץצץעקפץצץףףפףצרץפפספץסץץץפץפפףספפעףעסעףמענמעננננכנחוזוזךיךככממלנעסעץףפקפפצצרקצץרץפצשקצרשצרתקררקףש‎שתתשרששקרה₪I		-tֹלףץעץצפפףפץףץץצעעצץצפףץצצעףעץפנפצסץץעעפפעפננפפעףנסעסעסנףףנמסםמןםהווטהטךחחםכםנםנסנמנספצנףצעפקצששףצתפץתרתרקצקתץצר‎‎צשצתתשרקצך₪K
		0z־ננףרסעקפפקףצצפסרץעשץפפעצץץפקפץקעפרפףפפףרסספסעצמףעןסעסעסנמסןמעלןמהודזטיךחטככםלנממןנףףןסקןפץץץץקצרקרשצעפשתר‎תקשררקקש‏תרתרששעז¨N			1|ֽמעסףפפסקפץסעץקסצקעץץעפץעעקףץץצפצץצפמץצפעףףפפעףץנעעפסףנעסןםנןנמללגהזדחטוטכךלךםמםםמנןףףפפצףףקערשרקקקקרששקשקרשתשתש‎תקתתשץ‎רקץט¦N			1{ּםספףףףפקפסעףפצפפפקףץקפקצפפקפערץצפץףרעסףףףץסעץפנצמספםעףנןסנמןךםמכהבגחזהיוזיחלמכמןמןןמעפעפצץפצץשתפצףפץקעתפקש‎רצקרת‏תרשעוH	
2xּןףסצפצקפפקףפקפץרפץצץעףצץצףקקףפץףפףנצץעץצעףעףנפןפףףףףנעעןנןכןםםםלגבדוודוייייללםךננמסנסספסעץפצץףץקפשץץרשרשקררשקרתץשתקר‎רקצרץאF			2|ּםץעצעפצקצץץץצפפקצפפץפעףץפצפפצעעףצצצפףץעקץפפףעףןףעסןסננסןםנמםלמםךגאזהזהזטךחיךלםכלנםנןמףץףפצסץץרצרשקקצרששששתקששק‎תתשרשצקששתפשפב™C		2~ֿךףפףפץףצקץפפץפצפץץץץצףצקפפצנסףצפפץףץףצצפףףןפףספספנפןףסןןמךמללןללבגגגוהוטטךכחךללמנלנעןנףףעףףץפפררררצקתרשש‎ש‎שתפצרשתששתצץפא™C		2|ֽןנפץפצצףצפפףץץקפעצפפףץףץפפץרףרפפץצפץפעץפסףףסףףסעףףסןםסמםלמךלכלללאאבבטגזחיזכיכללנמנסננעפעףפפסץףץקצררתקרתצתת‎ששקששקתרשנב›E	3ֿםץףצףףקשסץץףפצףפץקפשעץצעץקףץפפףצץפסעץץספףפףפענץסנסןמןםמנמםניךכטכבאגדבזדחזזטלחכמםלסמסףספפנפצץקקףפקקקשקרתקרששקשתצשרשתשרצפא™E		3‚׀םףץץצצפפקץצץףפץפץץץץץףפקעףצפץץפץףףעצפףףףץפעעפןנןןננםננכםמםכמכךלידגגגהחיזיחטלךןממןמףססעףףףץסצצףפפרףתצררשרקרש‎תרקתקקשץנ˜D		4€ׂעףסצצפעפץצףפקףקצףפףפסקפצקפץרץצצצףעץפצענצעסצןספמסעמלןםןנלכםיככככטגדדדאזייטךיכןםןנמןעסעפעפקףצץעפקשצרפרצתצשקשר‎תתקן–C		3~־מץפקץקפץפקרפנרץץץףצפפצרץפפסץצצצףפףץףעץץספנףףנףףנןעמןנםמןלכליךםךם