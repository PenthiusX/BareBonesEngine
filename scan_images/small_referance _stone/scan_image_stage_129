	-pֱלפפפפץפפצפצפץףצנערפערץץקץףץפפףצפצעץץפצצנמפףפעףףצפעץסעפנפףןןסמןסןםנלםןגהגזחחזךחככךממללעןמפעפצףץפצףקקרצשררתתקרץששקרקתתתתשצפףך¯S
			+k»ינףץצספסקץעצקפףפףץץףץפצעשףפקעץפצפפפץקעץצפףףסעףףףפפסףףלענמענמנןמסםםללםבגוהחחזחחחטכללמכןןמעססעפסצץצקתקץקעפקץףקצקררצש‎תרתקתצט«S
'`´הנעפצץצפפצרפפץפץץעצצץץקףקףסצץעפקפסץץפץעערץףץפספנפנסנץעעענסעסמןמכנלכןךגבהחדוטזטחחםמטלסנםעמססףעףעפקפפץץץתףצקתשקרררשתצשרץשתרתרתו®R
	(a´דמצףפפצפצףצקץץצףץצףפצקץפעפףסקץףףףפףףעפקעץצףערעפףעסצןףףמעףםננןממלכלמכלדאהודזייייחךםםםםםלןםןעץףץףףצפרתץשקפרררשתצררשקתתשרשתץרפצך¬T	